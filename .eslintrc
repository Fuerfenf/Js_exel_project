// set params eslint
{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  // rules for project
  "rules": {
    "max-len": "off",
    "indent": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
    "padded-blocks": "off",
    "no-trailing-spaces": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}